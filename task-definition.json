{
  "family": "virtual-hendi",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "virtual-hendi-app",
      "image": "904336963348.dkr.ecr.us-east-1.amazonaws.com/virtual-hendi-main:latest",
      "cpu": 256,
      "memory": 512,
      "links": [""],
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "3000"
        }
      ],
      "privileged": true,
      "readonlyRootFilesystem": true,
      "interactive": true,
      "pseudoTerminal": true,
      "logConfiguration": {
        "logDriver": "syslog",
        "options": {
          "KeyName": ""
        }
      },
      "healthCheck": {
        "command": ["/health"],
        "interval": 1000,
        "timeout": 5000,
        "retries": 10,
        "startPeriod": 0
      }
    }
  ]
}
